{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "uuid": {
            "type": "string",
            "format": "uuid"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        },
        "expires_at": {
            "type": "string",
            "format": "date-time"
        },
        "token": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9]{60}$"
        }
    },
    "additionalProperties": false,
    "required": [
        "uuid",
        "created_at",
        "updated_at",
        "expires_at",
        "token"
    ]
}
